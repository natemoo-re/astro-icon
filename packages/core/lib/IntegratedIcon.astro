---
import Icon, { IconProps } from "./Icon.astro";
var virtual: typeof import("virtual:astro-icon");
try {
    virtual = await import("virtual:astro-icon");
} catch (_) {
    const err = new Error("Unable to load icons from plugin");
    if (import.meta.env.DEV) {
        err.hint =
            "Is the 'astro-icon' integration disabled?\n\nDid you meant to import 'astro-icon/standalone' instead of 'astro-icon/components'";
    }

    throw err;
}
const { default: icons, config } = virtual;

interface Props extends IconProps {}
---

<Icon icons={icons} config={config} {...Astro.props} />
